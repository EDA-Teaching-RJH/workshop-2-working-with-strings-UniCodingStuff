import math

def main():
   a = input("A:")
   b = input("B:")
   pythag(a,b)
   print(pythag(a,b))

def pythag(A,B):
    print(math.sqrt(A**2+B**2)) # sqrt comes from the math library link, ** comes from the lab script
    return (math.sqrt(A**2+B**2)) # return moves the output to where you called the function, so its moved this to be pythag(A, B)
main()
